{
  "_source": {
    "exclude": [
      "sfomuseum:description_embeddings"
    ]
  },
  "query": {
    "hybrid": {
      "queries": [
        {
          "match": {
            "text": {
              "query": "{QUERY}"
            }
          }
        },
        {
          "neural": {
            "sfomuseum:description_embeddings": {
              "query_text": "{QUERY}",
              "model_id": "{MODEL}",
              "k": 5
            }
          }
        }
      ]
    }
  }
}
